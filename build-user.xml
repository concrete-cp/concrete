<target name="help">
    <echo message="Please run: $ ant -v -projecthelp"/>
</target>

<target depends="init" name="buildCspfj">
        <echo message="${ant.project.name}: ${ant.file}"/>
        <javac destdir="bin">
            <src path="lib"/>
            <src path="src/cspfj"/>
            <classpath refid="project.classpath"/>
        </javac>
</target>

<target depends="clean,buildCspfj" name="makeCspfj">
        <jar destfile="cspfj.jar" basedir="bin" manifest="cspfj.manifest" index="true">
        	<indexjars path="lib/options.jar" />
        	<indexjars path="lib/jel.jar" />
        </jar>
</target>